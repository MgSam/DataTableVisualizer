<UserControl x:Class="DataTableVisualizerExtension.DataTableVisualizerToolWindowControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dataTableViewer="clr-namespace:DataTableViewer;assembly=ShineTools.DataTableViewer"
             Background="{DynamicResource VsBrush.Window}"
             Foreground="{DynamicResource VsBrush.WindowText}"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             Name="DataTableVisualizer"
>
  <Grid>
    <dataTableViewer:ViewerControl x:Name="DataTableViewer"/>
  </Grid>
</UserControl>
